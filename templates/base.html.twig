                                <div id="submenu-2" class="collapse submenu" style="">
                                    <ul class="nav flex-column">
                                        <li class="nav-item">
                                            <a class="nav-link" href="{{path('pools')}}">Pools</a>
                                        </li>
                                        <li class="nav-item">
                                            <a class="nav-link" href="cards.html">Security</a>
                                        </li>
                                        <li class="nav-item">
                                            <a class="nav-link" href="{{ path('network')}}">Network</a>
                                        </li>
                                        <li class="nav-item">
                                            <a class="nav-link" href="cards.html">Performance</a>
                                        </li>
                                    </ul>
                                </div>
                            </li>
                            <li class="nav-item">
                                <a class="nav-link" href="#" data-toggle="collapse" aria-expanded="false" data-target="#submenu-3" aria-controls="submenu-3"><i class="fas fa-fw fa-plug"></i>Maintenance</a>
                                <div id="submenu-3" class="collapse submenu" style="">
                                    <ul class="nav flex-column">
                                        <li class="nav-item">
                                            <a class="nav-link" href="{{path('reboot')}}">Reboot</a>
                                        </li>
                                        <li class="nav-item">
                                            <a class="nav-link" href="chart-c3.html">Facroty reset</a>
                                        </li>
                                    </ul>
                                </div>
                            </li>
                        
                            <li class="nav-item">
                                <a class="nav-link" href="{{path('overview')}}"><i class="fa fa-microchip"></i>Overview</a>
                            </li>
                            <li class="nav-item">
                                <a class="nav-link" href="{{path('app_logout')}}"><i class="fas fa-power-off"></i> Logout</a>
                            </li>
                        </ul>
                    </div>
                </nav>
            </div>
        </div>
        <!-- ============================================================== -->
        <!-- end left sidebar -->
        <!-- ============================================================== -->
        <!-- ============================================================== -->
        <!-- wrapper  -->
        <!-- ============================================================== -->
        <div class="dashboard-wrapper">
            <div class="container-fluid dashboard-content">
                <!-- ============================================================== -->
                <!-- pageheader -->
                <!-- ============================================================== -->
                <div class="row">
                    <div class="col-xl-12 col-lg-12 col-md-12 col-sm-12 col-12">
                        <div class="page-header">
                            <h2 class="pageheader-title">{{ block('title')}} </h2>
                            <div class="page-breadcrumb">
                                <nav aria-label="breadcrumb">
                                    <ol class="breadcrumb">
                                        <li class="breadcrumb-item"><a href="{{path('home')}}" class="breadcrumb-link">Dashboard</a></li>
                                        <li class="breadcrumb-item active" aria-current="page">{{ block('title')}}</li>
                                    </ol>
                                </nav>
                            </div>
                        </div>
                    </div>
                </div>
                <!-- ============================================================== -->
                <!-- end pageheader -->
                <!-- ============================================================== -->
                <div class="row">
                    <div class="col-xl-12 col-lg-12 col-md-12 col-sm-12 col-12">
                        <p>
                                {% for message in app.flashes('success') %}
                                    <div class="alert alert-success">
                                        <i class="fa fa-info-circle"></i>
                                        {{ message }}
                                    </div>
                                {% endfor %}
                                {% for message in app.flashes('danger') %}
                                    <div class="alert alert-danger">
                                        <i class="fa fa-info-square"></i>
                                        {{ message }}
                                    </div>
                                {% endfor %}
                            {% block body %}{% endblock %}
                        </p>
                    </div>
                </div>
            </div>
            <!-- ============================================================== -->
            <!-- footer -->
            <!-- ============================================================== -->
            <div class="footer fixed-bottom">
                <div class="container-fluid">
                    <div class="row">
                        <div class="col-xl-6 col-lg-6 col-md-12 col-sm-12 col-12">
